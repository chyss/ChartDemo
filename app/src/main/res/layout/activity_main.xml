    <com.chyss.chartdemo.ChartView
        android:id="@+id/main_chart"
        android:layout_marginTop="100dp"
        android:background="#F5F5F5"
        android:layout_width="match_parent"
        android:layout_height="200dp"/>

</RelativeLayout>
